{
	"Action": {
		"icon": {
			"Default": {
				"$type": "fontWeights",
				"$value": "{font.font-weight.body.medium}"
			}
		},
		"size": {
			"Default": {
				"$type": "number",
				"$value": "{system.dimension.size.sm}"
			}
		},
		"color": {
			"Default": {
				"$type": "color",
				"$value": "{color.bottle-green.bottle-green-700}"
			}
		},
		"border": {
			"stroke": {
				"Default": {
					"$type": "number",
					"$value": "{border.width.border-width-0}"
				}
			},
			"Default": {
				"$type": "color",
				"$value": "{system.border.container-color.primary}"
			}
		},
		"radius": {
			"corner": {
				"Default": {
					"$type": "number",
					"$value": "{system.border.radius.full}"
				}
			}
