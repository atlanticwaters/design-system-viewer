			"padding": {
				"Default": {
					"$type": "number",
					"$value": "{spacing.spacing-2}"
				}
			}
		},
		"background": {
			"fill": {
				"Default": {
					"$type": "color",
					"$value": "{system.background.container-color.white}"
				}
			},
			"Default": {
				"$type": "color",
				"$value": "{component.background.button-color.transparent-10.default}"
			}
		},
		"border-width": {
			"Default": {
				"$type": "color",
				"$value": "{component.border.selector-color.outline.selected-accent}"
			}
		}
