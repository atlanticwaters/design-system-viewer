{
	"Bottom-Actions": {
		"icon": {
			"Default": {
				"$type": "color",
				"$value": "{component.icon.button-color.orange-filled.default}"
			}
		},
		"size": {
			"Default": {
				"$type": "number",
				"$value": "{system.dimension.size.xl}"
			}
		},
		"radius": {
			"corner": {
				"Default": {
					"$type": "number",
					"$value": "{system.border.radius.full}"
				}
			}
		},
		"spacing": {
			"gap": {
				"Default": {
					"$type": "number",
					"$value": "{spacing.spacing-5}"
				}
			},
			"padding": {
				"Default": {
					"$type": "number",
					"$value": "{system.dimension.gap.2xs}"
				}
			}
		},
		"background": {
			"fill": {
				"Default": {
					"$type": "color",
					"$value": "{component.icon.button-color.orange-filled.default}"
				}
			},
			"Default": {
				"$type": "color",
				"$value": "{component.background.button-color.brand-filled.default}"
			}
		},
		"font-weight": {
			"Default": {
				"$type": "string",
				"$value": "{font.font-weight.body.bold}"
			}
		}
	}
}