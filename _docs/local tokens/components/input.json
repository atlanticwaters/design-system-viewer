{
	"Input": {
		"size": {
			"Default": {
				"$type": "number",
				"$value": "{spacing.spacing-9}"
			}
		},
		"color": {
			"Default": {
				"$type": "color",
				"$value": "{color.bottle-green.bottle-green-500}"
			}
		},
		"border": {
			"stroke": {
				"Default": {
					"$type": "color",
					"$value": "{component.border.input-color.default}"
				}
			},
			"Default": {
				"$type": "color",
				"$value": "{component.background.input-color.white-outlined.default}"
			}
		},
		"radius": {
			"corner": {
				"Default": {
					"$type": "number",
					"$value": "{system.border.radius.2xl}"
				}
			}
		},
		"string": {
			"Default": {
				"$type": "string",
				"$value": "{system.elevation.lowest}"
			}
		},
		"spacing": {
			"gap": {
				"Default": {
					"$type": "number",
					"$value": "{spacing.spacing-0}"
				}
			},
			"Default": {
				"$type": "number",
				"$value": "{spacing.spacing-6}"
			},
			"padding": {
				"Default": {
					"$type": "number",
					"$value": "{spacing.spacing-0}"
				}
			}
		},
		"background": {
			"fill": {
				"Default": {
					"$type": "color",
					"$value": "{color.bottle-green.bottle-green-500}"
				}
			}
		},
		"border-width": {
			"Default": {
				"$type": "number",
				"$value": "{system.border.width.xs}"
			}
		}
	}
}