{
	"Mini-Product-Card": {
		"icon": {
			"Default": {
				"$type": "color",
				"$value": "{component.icon.button-color.orange-filled.default}"
			}
		},
		"size": {
			"Default": {
				"$type": "number",
				"$value": "{font.font-size.body-sm}"
			}
		},
		"text": {
			"Default": {
				"$type": "color",
				"$value": "{system.text.on-surface-color.success-accent-1}"
			}
		},
		"border": {
			"stroke": {
				"Default": {
					"$type": "number",
					"$value": "{border.width.border-width-0}"
				}
			},
			"Default": {
				"$type": "color",
				"$value": "{system.border.container-color.primary}"
			}
		},
		"radius": {
			"corner": {
				"Default": {
					"$type": "number",
					"$value": "{system.border.radius.full}"
				}
			}
		},
		"spacing": {
			"gap": {
				"Default": {
					"$type": "number",
					"$value": "{spacing.spacing-5}"
				}
			},
			"Default": {
				"$type": "number",
				"$value": "{spacing.spacing-10}"
			},
			"padding": {
				"Default": {
					"$type": "number",
					"$value": "{spacing.spacing-0}"
				}
			}
		},
		"background": {
			"fill": {
				"Default": {
					"$type": "color",
					"$value": "{color.greige.greige-900}"
				}
			},
			"Default": {
				"$type": "color",
				"$value": "{component.background.button-color.brand-filled.default}"
			}
		},
		"font-weight": {
			"Default": {
				"$type": "fontWeights",
				"$value": "{font.font-weight.body.bold}"
			}
		},
		"border-width": {
			"Default": {
				"$type": "number",
				"$value": "{system.border.width.xs}"
			}
		}
	}
