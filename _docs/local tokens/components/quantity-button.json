			"Default": {
				"$type": "color",
				"$value": "{component.icon.input-color.inactive}"
			}
		},
		"size": {
			"Default": {
				"$type": "number",
				"$value": "{system.dimension.size.xl}"
			}
		},
		"color": {
			"Default": {
				"$type": "color",
				"$value": "{color.transparent-black.transparent-black-075}"
