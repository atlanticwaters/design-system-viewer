{
  "Quantity-Picker": {
    "icon": {
      "Active-Pressed": {
        "$type": "color",
        "$value": "{component.icon.input-color.default}"
      }
    },
    "size": {
      "Default": {
        "$type": "number",
        "$value": "{system.dimension.size.md}"
      },
      "Active-Pressed": {
        "$type": "number",
        "$value": "{system.dimension.size.sm}"
      }
    },
    "text": {
      "Default": {
        "$type": "color",
        "$value": "{component.text.input-color.transparent-10.default}"
      }
    },
    "color": {
      "Active-Pressed": {
        "$type": "color",
        "$value": "{color.transparent-black.transparent-black-075}"
      }
    },
    "radius": {
      "corner": {
        "Default": {
          "$type": "number",
          "$value": "{system.border.radius.none}"
        },
        "Active-Pressed": {
          "$type": "number",
          "$value": "{border.radius.border-radius-999}"
        }
      }
    },
    "spacing": {
      "gap": {
        "Default": {
          "$type": "number",
          "$value": "{spacing.spacing-2}"
        },
        "Active-Pressed": {
          "$type": "number",
          "$value": "{system.dimension.gap.none}"
        }
      },
      "Default": {
        "$type": "number",
        "$value": "{spacing.spacing-10}"
      },
      "padding": {
        "Default": {
          "$type": "number",
          "$value": "{spacing.spacing-3}"
        },
        "Active-Pressed": {
          "$type": "number",
          "$value": "{system.dimension.gap.2xs}"
        }
      },
      "Active-Pressed": {
        "$type": "number",
        "$value": "{spacing.spacing-10}"
      }
    },
    "background": {
      "fill": {
        "Default": {
          "$type": "color",
          "$value": "{component.text.input-color.transparent-10.default}"
        },
        "Active-Pressed": {
          "$type": "color",
          "$value": "{component.icon.input-color.default}"
        }
      },
      "Default": {
        "$type": "color",
        "$value": "{component.background.input-color.transparent-10.default}"
      }
    },
    "font-weight": {
      "Default": {
        "$type": "string",
        "$value": "{font.font-weight.body.bold}"
      },
      "Active-Pressed": {
        "$type": "string",
        "$value": "{font.font-weight.body.bold}"
      }
    },
    "border-width": {
      "Default": {
        "$type": "number",
        "$value": "{border.width.border-width-1}"
      },
      "Active-Pressed": {
        "$type": "number",
        "$value": "{border.width.border-width-1}"
      }
    }
  }
}