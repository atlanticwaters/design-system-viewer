{
  "Radio-Button": {
    "size": {
      "Default": {
        "$type": "number",
        "$value": "{system.dimension.size.xs}"
      }
    },
    "border": {
      "stroke": {
        "Default": {
          "$type": "color",
          "$value": "{component.border.selector-color.filled.selected}"
        }
      },
      "Default": {
        "$type": "color",
        "$value": "{component.border.selector-color.filled.selected}"
      }
    },
    "radius": {
      "corner": {
        "Default": {
          "$type": "number",
          "$value": "{system.border.radius.full}"
